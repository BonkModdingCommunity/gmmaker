{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
		"linebreak-style": "off",
		"max-len": "off",
		"prefer-rest-params": "off",
		"prefer-spread": "off"
    }
}
